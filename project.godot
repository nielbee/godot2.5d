config_version=5

[application]

config/name="godot2.5d"
run/main_scene="uid://pdmqxhdjhjcq"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Debugging="*res://scenes/UI/debugging.tscn"
MyPlayer="*res://scenes/Player.tscn"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogue_manager]

editor/new_file_template="~ this_is_a_node_title
Nathan: [[Hi|Hello|Howdy]], this is some dialogue.
Nathan: Here are some choices.
- First one
	Nathan: You picked the first one.
- Second one
	Nathan: You picked the second one.
- Start again => this_is_a_node_title
- End the conversation => END
Nathan: For more information see the online documentation.
=> END"
runtime/balloon_path="res://dialog/balloon.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/aspect="ignore"
window/stretch/scale=3.9

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

